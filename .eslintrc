{
	"extends":"eslint-config-airbnb",
	"env":{
		"browser": true,
    	"node": true
	},
	"settings":{
		"import/parser": "babel-eslint",
		"import/resolve": {
      		"moduleDirectory": ["node_modules", "src"]
    	}
	},
    "globals":{
    	"__DEV__":true,
    	"__PROD__":true,
    	"__CLIENT__":true,
    	"__SERVER__":true
    }
}
